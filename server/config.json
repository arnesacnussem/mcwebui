{
  "app": {
    "autoStartDaemon": true,
    "logViewer": {
      "default": "latest",
      "list": {
        "latest": "logs/latest.log",
        "debug": "logs/debug.log"
      }
    }
  },
  "daemon": {
    "cmd": "bash",
    "args": ["start.sh"],
    "cwd": "/workspace/server",
    "graceShutdown": {
      "command": "stop"
    }
  }
}
